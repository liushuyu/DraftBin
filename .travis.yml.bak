language: c
compiler:
  - gcc

    
script:
sudo add-apt-repository ppa:tobydox/mingw-x-precise -y
sudo apt-get update -qq
sudo apt-get install -y mingw32
git clone "https://github.com/ProjectCoolQ/cqsdk-vc.git"
cd cqsdk-vc
git am -3 "../0001-CMake-MinGW-Add-CMake-and-MinGW-support.patch"
sh build.sh
